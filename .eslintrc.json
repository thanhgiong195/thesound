{
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-unused-vars": 1,
    "no-var": 2,
    "constructor-super": 2,
    "no-const-assign": 2,
    "no-duplicate-imports": 2,
    "no-this-before-super": 2,
    "no-console": 0,
    "no-extra-boolean-cast": 0,
    "default-case": 1,
    "eqeqeq": 1,
    "no-multi-spaces": 1
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "arguments": true
  },
  "parser": "babel-eslint"
}